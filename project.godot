config_version=4

[application]

config/name="lelcubeOS"
config/description="A game about using a computer to start a revolution lol"
run/main_scene="res://OS/Core/MobileSetup.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="lelcubeOS"
boot_splash/image="res://Assets/Boot/Splash.png"
boot_splash/fullsize=false
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://icon.png"
config/quit_on_go_back=false

[autoload]

SavingManager="*res://OS/Scripts/SavingManager/SavingManager.cs"
LelfsManager="*res://OS/Scripts/Lelfs/LelfsManager.cs"
ResolutionManager="*res://OS/Scripts/Drivers/ResolutionManager.cs"
WindowManager="*res://OS/Scripts/WindowManager/WindowManager.cs"
SoundManager="*res://OS/Scripts/Drivers/SoundManager.cs"
NotificationManager="*res://OS/Scripts/Lelsktop/Overlay/NotificationManager.cs"

[display]

window/size/width=1280
window/size/height=720
window/size/fullscreen=true
window/handheld/orientation="sensor_landscape"
window/stretch/mode="2d"

[gui]

common/drop_mouse_on_gui_input_disabled=true

